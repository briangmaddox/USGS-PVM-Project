[Config for BinMakefileAm]
addcxxflags=-DTNT_NO_BOUNDS_CHECK
bin_program=pvmproject
cxxflags=\s-O0 -g3 -Wall
ldadd=-lProjectionMesh -lMathLib -lProjectionIO -lImageLib  -lgeotiff -ltiff -lProjection  -lgctpc -lMiscUtils -lpvm3 -lACE
