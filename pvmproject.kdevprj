[General]
AMChanged=false
author=CHris Bilderback, Keith Athmer and Brian Maddox
configure_args=--prefix=/home/cbilder
email=cbilder@mcpc1159
kdevprj_version=1.3
lfv_open_groups=
make_options=\s-j1
makefiles=Makefile.am,pvmproject/Makefile.am,pvmproject/docs/Makefile.am,pvmproject/docs/en/Makefile.am
modifyMakefiles=true
project_name=PVMProject
project_type=normal_cpp
